{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["> 1%", "last 2 versions"],
          "node": "6"
        }
      }
    ]
  ],
  "plugins": [
    ["transform-runtime", {
      "helpers": true,
      "polyfill": false,
      "regenerator": false,
      "moduleName": "babel-runtime"
    }],
    ["add-module-exports"]
  ]
}